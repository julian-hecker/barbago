{
  "expo": {
    "name": "Barbago App",
    "slug": "barbago-app",
    "scheme": "barbago",
    "version": "1.0.0",
    "entryPoint": "src/App.tsx",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "build": {
        "babel": {
          "include": ["react-router-native", "react-leaflet"]
        }
      },
      "favicon": "./assets/favicon.png"
    },
    "description": "",
    "githubUrl": "https://github.com/julian-hecker/barbago"
  }
}
